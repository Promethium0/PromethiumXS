START:
    MOV 10 R1          ; Set R1 = 10
    ADDI 5 R1          ; Add 5 to R1 (R1 = 15)
    CALL SUBROUTINE ;call the subroutine function
    JMP END            ; Jump to the END label

SUBROUTINE:
    ADDI 20 R1         ; Add 20 to R1 (R1 = R1 + 20)
    MOV 5 R2           ; Set R2 = 5
    RET                ; Return to the caller

END:
    HLT                ; Halt the program
